{
  "name": "class-checkin",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend dev:face-recognition",
    "build": "tsc && vite build",
    "dev:backend": "convex dev",
    "dev:frontend": "vite --open",
    "dev:face-recognition": "cd face-recognition && python main.py",
    "predev": "convex dev --once"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@tailwindcss/vite": "^4.0.15",
    "convex": "1.21.0",
    "javascript-time-ago": "^2.5.11",
    "prettier": "3.4.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-time-ago": "^7.3.3",
    "tailwindcss": "^4.0.15"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/node": "^18.17.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "npm-run-all2": "^7.0.0",
    "typescript": "~5.0.3",
    "vite": "^5.4.12"
  }
}
